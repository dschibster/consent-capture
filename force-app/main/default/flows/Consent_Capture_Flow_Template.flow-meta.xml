    <description>Consent Capture allows you to give your users the power to create Contact Point Type Consent Records directly from their Lead and Contact records!</description>
    <formulas>
        <description>The concatenation of the Contact Point Type Name &amp; Data Use Purpose Name</description>
        <name>contactPointTypeConsentName</name>
        <dataType>String</dataType>
        <expression>{!contactPointConsentType}&amp;&quot; - &quot;&amp;{!dataUsePurposeName}</expression>
    </formulas>
    <formulas>
        <description>Calculate the effective to date by an average of 30 days per month multiplied by the month</description>
        <name>effectiveTo</name>
        <dataType>DateTime</dataType>
        <expression>{!$Flow.CurrentDateTime} + ({!effectiveToInMonths}*30)</expression>
    </formulas>
